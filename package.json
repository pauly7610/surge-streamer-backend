{
  "name": "surge-streamer-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Real-time data streaming system for Uber Surge Prediction",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development node dist/index.js",
    "start:staging": "cross-env NODE_ENV=staging node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "tsc",
    "build:clean": "rimraf dist && tsc",
    "deploy": "supabase functions deploy",
    "start:pipeline": "node dist/services/startPipeline.js",
    "lint": "tsc --noEmit",
    "test": "jest",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "kafka:start": "docker-compose up -d kafka zookeeper",
    "kafka:stop": "docker-compose down",
    "train-model": "ts-node src/ml/trainModel.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "@tensorflow/tfjs-node": "^4.17.0",
    "apollo-server-express": "^3.13.0",
    "avro-js": "^1.11.3",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.18.3",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.15.0",
    "h3-js": "^4.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "ml-matrix": "^6.11.0",
    "ml-random-forest": "^2.1.0",
    "ml-regression": "^5.0.0",
    "mongodb": "^6.4.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.13",
    "rxjs": "^7.8.1",
    "subscriptions-transport-ws": "^0.11.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "cross-env": "^7.0.3",
    "docker-compose": "^0.24.6",
    "eslint": "^9.22.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "supabase": "^1.142.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.26.1"
  }
}
