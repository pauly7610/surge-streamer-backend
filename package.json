{
  "name": "surge-streamer-backend",
  "private": true,
  "version": "0.1.0",
  "type": "commonjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development node dist/index.js",
    "start:staging": "cross-env NODE_ENV=staging node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "tsc",
    "build:clean": "rimraf dist && tsc",
    "deploy": "supabase functions deploy",
    "start:pipeline": "node dist/services/startPipeline.js",
    "lint": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test jest",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "rxjs": "^7.8.1",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "uuid": "^9.0.1",
    "h3-js": "^4.1.0",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "supabase": "^1.142.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
