# Environment Configuration Example
# Copy this file to .env, .env.development, .env.staging, .env.production, or .env.test
# and fill in the appropriate values for your environment

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Node Environment (development, production, test, staging)
NODE_ENV=development

# Server Configuration
PORT=3000
HOST=localhost

# Logging
# Levels: error, warn, info, http, verbose, debug, silly
LOG_LEVEL=info

# Geospatial Settings
DEFAULT_H3_RESOLUTION=8
DEFAULT_BOUNDING_BOX_RADIUS_KM=10

# Data Pipeline Configuration
PIPELINE_INTERVAL_MS=60000
MAX_BATCH_SIZE=1000
CACHE_TTL_SECONDS=300

# Performance Settings
WORKER_THREADS=1
MAX_CONNECTIONS=100

# Debug Settings (development/test only)
# DEBUG=surge-streamer:*
# DEBUG_COLORS=true

# Test Settings (test only)
# TEST_TIMEOUT_MS=5000
# MOCK_EXTERNAL_SERVICES=true

# Monitoring (staging/production only)
# ENABLE_PERFORMANCE_MONITORING=true
# ENABLE_ERROR_REPORTING=true 